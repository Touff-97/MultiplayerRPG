config_version=4

[application]

config/name="MMORPG_Client"
run/main_scene="res://Scenes/MainScenes/SceneHandler.tscn"
config/icon="res://icon.png"

[autoload]

Server="*res://Scenes/Singletons/Server.gd"
Gateway="*res://Scenes/Singletons/Gateway.gd"

[debug]

multirun/number_of_windows=2
multirun/window_distance=1270
multirun/add_custom_args=true
multirun/first_window_args="listen"
multirun/other_window_args="join"

[display]

window/size/width=1280
window/size/height=720
window/size/test_width=1280
window/size/test_height=720

[editor_plugins]

enabled=PoolStringArray( "res://addons/multirun/plugin.cfg", "res://addons/scatter/plugin.cfg" )

